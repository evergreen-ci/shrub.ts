    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "chai": "^4.2.0",
    "gts": "^1.0.0",
    "install": "^0.12.2",
    "mocha": "^6.1.4",
    "npm": "^6.9.0",
    "ts-node": "^8.1.0",
    "ts-protoc-gen": "^0.9.1-pre.cbe7398",
