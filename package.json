{
  "name": "evergreen-shrub",
  "version": "0.1.3",
  "description": "An Evergreen project generation library",
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": ". scripts/compile.sh",
    "fix": "gts fix",
    "prepare": "npm run compile",
